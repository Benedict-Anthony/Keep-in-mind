    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "extra": {
      "eas": {
        "projectId": "1eb9a8a2-9123-476b-a2bd-f39e8b59d225"
      }
    },
    "owner": "benwebdev"
  }
}
